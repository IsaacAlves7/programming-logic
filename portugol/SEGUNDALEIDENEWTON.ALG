Algoritmo "segundaleidenewton"

Var
   m,a,F:Real
Inicio
   Escreva("[m] Digite o valor da massa(g):")
   Leia(m)
   Escreva("[a] Digite o valor da aceleração(m/s^2):")
   Leia(a)
   F<-m*a
   Escreval("O valor da força é de",F," N")
   Escreva("O que sabemos é uma gota, o que desprezamos é um oceano-Isaac Newton ")

Fimalgoritmo